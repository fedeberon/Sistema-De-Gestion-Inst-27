 CREATE TABLE `usuarios` (
    `USERNAME` varchar(50) NOT NULL,
    `PASSWORD` varchar(45) NOT NULL,
    `NOMBRE` varchar(255) DEFAULT NULL,
    `APELLIDO` varchar(255) DEFAULT NULL,
    `DNI` varchar(45) DEFAULT NULL,
    `ALU_FECHA_DE_NACIMIENTO` varchar(45) DEFAULT NULL,
    `ALU_CELULAR` varchar(45) DEFAULT NULL,
    `ALU_DIRECCION` varchar(45) DEFAULT NULL,
    `ALU_EMAIL` varchar(45) DEFAULT NULL,
    `PRO_DIRECCION` varchar(45) DEFAULT NULL,
    `PRO_NUMERO` int(4) DEFAULT NULL,
    `PRO_CELULAR` int(45) DEFAULT NULL,
    `PRO_FIJO` int(45) DEFAULT NULL,
    `PRO_CUIL` int(11) DEFAULT NULL,
    `PRO_NACIMIENTO date DEFAULT NULL,
    `DTYPE` varchar(45) DEFAULT NULL,
    PRIMARY KEY (`USERNAME`)
  );